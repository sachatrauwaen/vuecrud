{
    "name": "vuecrud",
    "version": "0.2.192",
    "private": true,
    "main": "dist/vuecrud.umd.js",
    "scripts": {
        "serve": "vue-cli-service serve src/demo/main.js",
        "build": "vue-cli-service build --target lib --name vuecrud src/lib/index.js --env.patch",
        "lint": "vue-cli-service lint",
        "build-patch": "vue-cli-service build --target lib --name vuecrud src/lib/index.js --env.patch",
        "build-minor": "vue-cli-service build --target lib --name vuecrud src/lib/index.js --env.minor",
        "build-major": "vue-cli-service build --target lib --name vuecrud src/lib/index.js --env.major"
    },
    "dependencies": {
        "element-ui": "^2.15.5",
        "json-ref-lite": "^1.1.0",
        "vue": "^2.6.14",
        "vue-router": "^3.5.2"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.0-rc.5",
        "@vue/cli-plugin-eslint": "^3.0.0-rc.6",
        "@vue/cli-service": "^3.0.0-rc.6",
        "@vue/eslint-config-standard": "^3.0.0-rc.5",
        "vue-template-compiler": "^2.6.14",
        "webpack-auto-inject-version": "^1.1.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}