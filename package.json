{
  "name": "vuecrud",
  "version": "0.2.187",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve src/demo/main.js",
    "build": "vue-cli-service build --target lib --name vuecrud src/lib/index.js --env.patch",
    "lint": "vue-cli-service lint",
    "build-major": "vue-cli-service build --target lib --name vuecrud src/lib/index.js --env.major",
    "build-minor": "vue-cli-service build --target lib --name vuecrud src/lib/index.js --env.minor",
    "build-patch": "vue-cli-service build --target lib --name vuecrud src/lib/index.js --env.patch"
  },
  "main": "dist/vuecrud.umd.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.15.5",
    "json-ref-lite": "^1.1.0",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.14",
    "webpack-auto-inject-version": "^1.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
